                    <ul class="list-unstyled footer-list-style cu-footer-items menu-close">
                      {% for link in footer_menu_2.links %}
                        <li>
                          <a href="{{ link.url }}">{{ link.title }} </a>
                        </li>
                      {% endfor %}
                    </ul>

                    <script>
  function toggleMenu(element) {
    var menu = element.nextElementSibling;
    var plusIcon = element.querySelector('.plus-icon');
    if (window.innerWidth < 768) { // Check if the screen width is less than 768px (mobile view)
      if (menu.style.display === "none" || menu.style.display === "") {
        menu.style.display = "block";
        plusIcon.textContent = "−"; // Change plus to minus when expanded
      } else {
        menu.style.display = "none";
        plusIcon.textContent = "+"; // Change minus back to plus when collapsed
      }
    }
  }
