            <div class="panel panel-default">
              <div class="panel-heading" role="tab" id="headingOne">
                <h4 class="panel-title">
                  <a
                    role="button"
                    data-toggle="collapse"
                    data-parent="#accordion"
                    href="#collapseOne"
                    aria-expanded="true"
                    aria-controls="collapseOne"
                    class=""
                    >Description</a
                  >
                </h4>
              </div>
              <div
                id="collapseOne"
                class="panel-collapse collapse in"
                role="tabpanel"
                aria-labelledby="headingOne"
                aria-expanded="true"
              >
                <div class="panel-body">
                  {{ product.description }}
                </div>
              </div>
            </div>
            {% if section.settings.enable_shipping %}
              <div class="panel panel-default">
                <div class="panel-heading" role="tab" id="custom-tab">
                  <h4 class="panel-title">
                    <a
                      role="button"
                      data-toggle="collapse"
                      data-parent="#accordion"
                      href="#custom-tabs"
                      aria-expanded="false"
                      aria-controls="review-fix"
                      class="collapsed"
                    >
                      {% if section.settings.shipping_content != blank %}
                        {{ pages[section.settings.shipping_content].title }}
                      {% else %}
                        Page Title
                      {% endif %}
                    </a>
                  </h4>
                </div>
                <div
                  id="custom-tabs"
                  class="panel-collapse collapse"
                  role="tabpanel"
                  aria-labelledby="review-fix"
                  aria-expanded="false"
                >
                  <div class="panel-body">
                    {% if section.settings.shipping_content != blank %}
                      {{ pages[section.settings.shipping_content].content }}
                    {% else %}
                      <p class="col-md-12">
                        Please select a Page from <a href="/admin/themes" class="text-muted">theme customization</a>
                      </p>
                    {% endif %}
                  </div>
                </div>
              </div>
            {% endif %}
            {% if settings.enable_review %}
              <div class="panel panel-default">
                <div class="panel-heading" role="tab" id="review-fix">
                  <h4 class="panel-title">
                    <a
                      role="button"
                      data-toggle="collapse"
                      data-parent="#accordion"
                      href="#review"
                      aria-expanded="false"
                      aria-controls="review-fix"
                      class="collapsed"
                    >
                      {{- 'products.template.customer_reviews' | t -}}
