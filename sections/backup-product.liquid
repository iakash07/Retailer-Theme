                      {{ product.metafields.spr.reviews }}
                    </div>
                  </div>
                </div>
              </div>
            {% endif %}
            {% if section.settings.enable_custom_page %}
              <div class="panel panel-default">
                <div class="panel-heading" role="tab" id="shipping-fix">
